{
  "name": "sample-node-typescript",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "main": "src/server.js",
  "scripts": {
    "build-ts": "tsc -p src",
    "build": "npm run util:clean && npm run migrate && npm run build-ts",
    "migrate": "knex migrate:latest",
    "test": "cross-env NODE_ENV=test TS_NODE_PROJECT=test mocha --reporter spec --compilers ts:ts-node/register -r tsconfig-paths/register test/unit/**/*.test.ts",
    "tslint": "tslint -c tslint.json -p src/tsconfig.json",
    "watch-ts": "tsc -w -p src",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Express\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "util:clean": "rimraf dist",
    "serve": "cross-env nodemon dist/index.js",
    "serve:prod": "cross-env node dist/index.js",
    "start:dev": "npm run build && npm run watch",
    "start:prod": "npm run build && npm run serve:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hdkhardikB/sample-node-typescript.git"
  },
  "keywords": [
    "typescript",
    "express"
  ],
  "author": "hdkhardikb <hardikdce7008@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^6.5.3",
    "agenda": "^2.3.0",
    "aws-sdk": "^2.627.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.1",
    "bookshelf": "^0.10.4",
    "bookshelf-uuid": "^1.0.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "debug": "^3.2.6",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "express-limiter": "^1.6.1",
    "fs": "0.0.1-security",
    "helmet": "^3.22.0",
    "html-pdf": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.4",
    "lusca": "^1.5.1",
    "mailgun-js": "^0.22.0",
    "moment": "^2.24.0",
    "mongoose": "^5.9.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^1.4.2",
    "nodemailer": "^6.3.0",
    "pg": "^7.17.0",
    "socket.io": "^2.3.0",
    "sqlite3": "^4.1.1",
    "stripe": "^8.14.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "uuid": "^3.3.3",
    "validatorjs": "^3.13.5"
  },
  "devDependencies": {
    "@types/agenda": "^2.0.6",
    "@types/body-parser": "^1.16.5",
    "@types/bookshelf": "^0.9.6",
    "@types/chai": "^4.0.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^4.0.1",
    "@types/express": "^4.0.37",
    "@types/knex": "0.0.61",
    "@types/mocha": "^2.2.43",
    "@types/morgan": "^1.7.32",
    "@types/multer": "^1.3.10",
    "@types/node": "^8.0.28",
    "@types/stripe": "^7.13.21",
    "@types/supertest": "^2.0.3",
    "@types/validatorjs": "^3.7.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@types/mongoose": "^5.7.10",
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
